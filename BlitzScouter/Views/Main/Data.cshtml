@model BlitzScouter.Models.BSRaw
@{
	ViewData["Title"] = "Send Data";
}

@using (Html.BeginForm("Index", "Main", FormMethod.Post))
{
	<button type="button" id="header" onclick="document.documentElement.requestFullscreen()">Uploaded!</button>
	<div id="content">
		<div class="center">
			<div class="title">You've Scouted</div>
		</div>
		<div class="center">
			<div class="title" id="scoreNum"></div>
		</div>
		<div class="center">
			<div class="title">Times</div>
		</div>
		<input type="submit" id="footer" value="Return" />
	</div>
}
<script>loadScore();</script>
